{
  "name": "line_lead_qsr_mvp",
  "version": "1.0.0",
  "private": true,
  "description": "Line Lead QSR Assistant - Mobile-first chat interface",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,json,css,md}",
    "lint:python": "python3 -m flake8 *.py backend/*.py",
    "format:python": "python3 -m black *.py backend/*.py && python3 -m isort *.py backend/*.py",
    "prepare": "husky install"
  },
  "keywords": [
    "qsr",
    "assistant",
    "chat",
    "mobile"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@assistant-ui/react": "^0.10.24",
    "@assistant-ui/styles": "^0.1.13",
    "lucide-react": "^0.523.0",
    "pdfjs-dist": "^5.3.31",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-pdf": "^10.0.1",
    "react-scripts": "^5.0.1",
    "remark-gfm": "^4.0.1"
  },
  "proxy": "http://localhost:8000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "eslint": "^8.57.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2"
  }
}
